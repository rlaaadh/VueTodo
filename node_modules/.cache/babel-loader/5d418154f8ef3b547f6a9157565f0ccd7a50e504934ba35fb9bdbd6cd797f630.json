{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport TodoHeader from './components/TodoHeader.vue';\nimport TodoInput from './components/TodoInput.vue';\nimport TodoList from './components/TodoList.vue';\nimport TodoFooter from './components/TodoFooter.vue';\nexport default {\n  data: function () {\n    return {\n      todoItems: []\n    };\n  },\n  methods: {\n    addOneItem: function (todoItem) {\n      var obj = {\n        completed: false,\n        item: todoItem\n      };\n      localStorage.setItem(todoItem, JSON.stringify(obj));\n      this.todoItems.push(obj);\n    },\n    removeOneItem: function (todoItem, index) {\n      localStorage.removeItem(todoItem.item);\n      this.todoItems.splice(index, 1);\n    },\n    toggleOneItem: function (index) {\n      // completed 속성에 true라면 false로, false라면 true로 변환\n      // todoItem.completed = !todoItem.completed;\n      this.todoitems[index].completed = !this.todoitems[index].completed;\n      localStorage.removeItem(todoItem.item);\n      localStorage.setItem(todoItem.item, JSON.stringify(todoItem));\n    }\n  },\n  created: function () {\n    if (localStorage.length > 0) {\n      for (var i = 0; i < localStorage.length; i++) {\n        if (localStorage.key(i) != 'loglevel:webpack-dev-serve') {\n          this.todoItems.push(JSON.parse(localStorage.getItem(localStorage.key(i))));\n        }\n      }\n    }\n  },\n  name: 'App',\n  components: {\n    // 컴포넌트 태그명 : 컴포넌트 내용\n    'TodoHeader': TodoHeader,\n    'TodoInput': TodoInput,\n    'TodoList': TodoList,\n    'TodoFooter': TodoFooter\n  }\n};","map":{"version":3,"names":["TodoHeader","TodoInput","TodoList","TodoFooter","data","todoItems","methods","addOneItem","todoItem","obj","completed","item","localStorage","setItem","JSON","stringify","push","removeOneItem","index","removeItem","splice","toggleOneItem","todoitems","created","length","i","key","parse","getItem","name","components"],"sources":["src/App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <TodoHeader/>\n    <TodoInput @addTodoItem=\"addOneItem\"/>\n    <TodoList \n    :propsdata=\"todoItems\" \n    @removeItem=\"removeOneItem\"\n    @toggleItem=\"toggleOneItem\"/>\n    <TodoFooter/>\n  </div>\n</template>\n\n<script>\nimport TodoHeader from './components/TodoHeader.vue'\nimport TodoInput from './components/TodoInput.vue'\nimport TodoList from './components/TodoList.vue'\nimport TodoFooter from './components/TodoFooter.vue'\n\nexport default {\n  data: function(){\n    return{\n      todoItems: [],\n    }\n  },\n  methods:{\n    addOneItem: function(todoItem){\n      var obj = {completed: false, item: todoItem};\n      localStorage.setItem(todoItem, JSON.stringify(obj));\n      this.todoItems.push(obj);\n    },\n    removeOneItem: function(todoItem, index){\n      localStorage.removeItem(todoItem.item);\n      this.todoItems.splice(index, 1);\n    },\n    toggleOneItem: function(index){\n      // completed 속성에 true라면 false로, false라면 true로 변환\n      // todoItem.completed = !todoItem.completed;\n      this.todoitems[index].completed = !this.todoitems[index].completed;\n      \n      localStorage.removeItem(todoItem.item);\n      localStorage.setItem(todoItem.item, JSON.stringify(todoItem));\n    }\n  },\n  created: function(){\n    if(localStorage.length > 0){\n      for(var i=0; i < localStorage.length; i++){\n        if(localStorage.key(i) != 'loglevel:webpack-dev-serve'){\n          this.todoItems.push(JSON.parse(localStorage.getItem(localStorage.key(i))));\n        }\n      }\n    }\n  },\n  name: 'App',\n  components: {\n    // 컴포넌트 태그명 : 컴포넌트 내용\n    'TodoHeader' : TodoHeader,\n    'TodoInput' : TodoInput,\n    'TodoList' : TodoList,\n    'TodoFooter' : TodoFooter,\n  }\n}\n</script>\n\n<style>\n  body{\n    text-align: center;\n    background-color:#f6f6f6;\n  }\n  input{\n    border-style: groove;\n    width: 200px;\n  }\n  button{\n    border-style: groove;\n  }\n  .shadow{\n    box-shadow: 5px 10px 10px rgba(0,0,0,0.03);\n  }\n</style>\n"],"mappings":";AAaA,OAAAA,UAAA;AACA,OAAAC,SAAA;AACA,OAAAC,QAAA;AACA,OAAAC,UAAA;AAEA;EACAC,IAAA,WAAAA,CAAA;IACA;MACAC,SAAA;IACA;EACA;EACAC,OAAA;IACAC,UAAA,WAAAA,CAAAC,QAAA;MACA,IAAAC,GAAA;QAAAC,SAAA;QAAAC,IAAA,EAAAH;MAAA;MACAI,YAAA,CAAAC,OAAA,CAAAL,QAAA,EAAAM,IAAA,CAAAC,SAAA,CAAAN,GAAA;MACA,KAAAJ,SAAA,CAAAW,IAAA,CAAAP,GAAA;IACA;IACAQ,aAAA,WAAAA,CAAAT,QAAA,EAAAU,KAAA;MACAN,YAAA,CAAAO,UAAA,CAAAX,QAAA,CAAAG,IAAA;MACA,KAAAN,SAAA,CAAAe,MAAA,CAAAF,KAAA;IACA;IACAG,aAAA,WAAAA,CAAAH,KAAA;MACA;MACA;MACA,KAAAI,SAAA,CAAAJ,KAAA,EAAAR,SAAA,SAAAY,SAAA,CAAAJ,KAAA,EAAAR,SAAA;MAEAE,YAAA,CAAAO,UAAA,CAAAX,QAAA,CAAAG,IAAA;MACAC,YAAA,CAAAC,OAAA,CAAAL,QAAA,CAAAG,IAAA,EAAAG,IAAA,CAAAC,SAAA,CAAAP,QAAA;IACA;EACA;EACAe,OAAA,WAAAA,CAAA;IACA,IAAAX,YAAA,CAAAY,MAAA;MACA,SAAAC,CAAA,MAAAA,CAAA,GAAAb,YAAA,CAAAY,MAAA,EAAAC,CAAA;QACA,IAAAb,YAAA,CAAAc,GAAA,CAAAD,CAAA;UACA,KAAApB,SAAA,CAAAW,IAAA,CAAAF,IAAA,CAAAa,KAAA,CAAAf,YAAA,CAAAgB,OAAA,CAAAhB,YAAA,CAAAc,GAAA,CAAAD,CAAA;QACA;MACA;IACA;EACA;EACAI,IAAA;EACAC,UAAA;IACA;IACA,cAAA9B,UAAA;IACA,aAAAC,SAAA;IACA,YAAAC,QAAA;IACA,cAAAC;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}
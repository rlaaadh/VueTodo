 var obj = {completed: false, item: this.newTodoItem};\n      localStorage.setItem(this.newTodoItem, JSON.stringify(obj));\n    }\n  },\n  created: function(){\n    if(localStorage.length > 0){\n      for(var i=0; i < localStorage.length; i++){\n        if(localStorage.key(i) != 'loglevel:webpack-dev-serve'){\n          this.todoItems.push(JSON.parse(localStorage.getItem(localStorage.key(i))));\n        }\n      }\n    }\n  },\n  name: 'App',\n  components: {\n    // 컴포넌트 태그명 : 컴포넌트 내용\n    'TodoHeader' : TodoHeader,\n    'TodoInput' : TodoInput,\n    'TodoList' : TodoList,\n    'TodoFooter' : TodoFooter,\n  }\n}\n</script>\n\n<style>\n  body{\n    text-align: center;\n    background-color:#f6f6f6;\n  }\n  input{\n    border-style: groove;\n    width: 200px;\n  }\n  button{\n    border-style: groove;\n  }\n  .shadow{\n    box-shadow: 5px 10px 10px rgba(0,0,0,0.03);\n  }\n</style>\n"],"mappings":";AAUA,OAAAA,UAAA;AACA,OAAAC,SAAA;AACA,OAAAC,QAAA;AACA,OAAAC,UAAA;AAEA;EACAC,IAAA,WAAAA,CAAA;IACA;MACAC,SAAA;MACAC,WAAA;IACA;EACA;EACAC,OAAA;IACAC,UAAA,WAAAA,CAAA;MACA,IAAAC,GAAA;QAAAC,SAAA;QAAAC,IAAA,OAAAL;MAAA;MACAM,YAAA,CAAAC,OAAA,MAAAP,WAAA,EAAAQ,IAAA,CAAAC,SAAA,CAAAN,GAAA;IACA;EACA;EACAO,OAAA,WAAAA,CAAA;IACA,IAAAJ,YAAA,CAAAK,MAAA;MACA,SAAAC,CAAA,MAAAA,CAAA,GAAAN,YAAA,CAAAK,MAAA,EAAAC,CAAA;QACA,IAAAN,YAAA,CAAAO,GAAA,CAAAD,CAAA;UACA,KAAAb,SAAA,CAAAe,IAAA,CAAAN,IAAA,CAAAO,KAAA,CAAAT,YAAA,CAAAU,OAAA,CAAAV,YAAA,CAAAO,GAAA,CAAAD,CAAA;QACA;MACA;IACA;EACA;EACAK,IAAA;EACAC,UAAA;IACA;IACA,cAAAxB,UAAA;IACA,aAAAC,SAAA;IACA,YAAAC,QAAA;IACA,cAAAC;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}
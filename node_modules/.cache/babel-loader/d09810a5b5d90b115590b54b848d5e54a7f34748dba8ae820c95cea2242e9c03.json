eted: false, item: todoItem};\n      localStorage.setItem(todoItem, JSON.stringify(obj));\n      this.todoItems.push(obj);\n    }\n  },\n  created: function(){\n    if(localStorage.length > 0){\n      for(var i=0; i < localStorage.length; i++){\n        if(localStorage.key(i) != 'loglevel:webpack-dev-serve'){\n          this.todoItems.push(JSON.parse(localStorage.getItem(localStorage.key(i))));\n        }\n      }\n    }\n  },\n  name: 'App',\n  components: {\n    // 컴포넌트 태그명 : 컴포넌트 내용\n    'TodoHeader' : TodoHeader,\n    'TodoInput' : TodoInput,\n    'TodoList' : TodoList,\n    'TodoFooter' : TodoFooter,\n  }\n}\n</script>\n\n<style>\n  body{\n    text-align: center;\n    background-color:#f6f6f6;\n  }\n  input{\n    border-style: groove;\n    width: 200px;\n  }\n  button{\n    border-style: groove;\n  }\n  .shadow{\n    box-shadow: 5px 10px 10px rgba(0,0,0,0.03);\n  }\n</style>\n"],"mappings":";AAUA,OAAAA,UAAA;AACA,OAAAC,SAAA;AACA,OAAAC,QAAA;AACA,OAAAC,UAAA;AAEA;EACAC,IAAA,WAAAA,CAAA;IACA;MACAC,SAAA;IACA;EACA;EACAC,OAAA;IACAC,UAAA,WAAAA,CAAAC,QAAA;MACA,IAAAC,GAAA;QAAAC,SAAA;QAAAC,IAAA,EAAAH;MAAA;MACAI,YAAA,CAAAC,OAAA,CAAAL,QAAA,EAAAM,IAAA,CAAAC,SAAA,CAAAN,GAAA;MACA,KAAAJ,SAAA,CAAAW,IAAA,CAAAP,GAAA;IACA;EACA;EACAQ,OAAA,WAAAA,CAAA;IACA,IAAAL,YAAA,CAAAM,MAAA;MACA,SAAAC,CAAA,MAAAA,CAAA,GAAAP,YAAA,CAAAM,MAAA,EAAAC,CAAA;QACA,IAAAP,YAAA,CAAAQ,GAAA,CAAAD,CAAA;UACA,KAAAd,SAAA,CAAAW,IAAA,CAAAF,IAAA,CAAAO,KAAA,CAAAT,YAAA,CAAAU,OAAA,CAAAV,YAAA,CAAAQ,GAAA,CAAAD,CAAA;QACA;MACA;IACA;EACA;EACAI,IAAA;EACAC,UAAA;IACA;IACA,cAAAxB,UAAA;IACA,aAAAC,SAAA;IACA,YAAAC,QAAA;IACA,cAAAC;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}